      "format": "localizedString"
    },
    "_acronym": {
      "title": "t(study.acronym)",
      "type": "object",
      "format": "localizedString"
    },
    "_objectives": {
      "title": "t(study.objectives)",
      "type": "object",
      "format": "obibaSimpleMde"
    },
    "_opal": {
      "title": "t(study.opal)",
